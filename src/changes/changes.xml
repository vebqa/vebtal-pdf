<document xmlns="http://maven.apache.org/changes/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
	<properties>
		<title>VEBTAL PDF - Test Adaption Layer</title>
		<author email="kdoerges@googlemail.com">Karsten Doerges</author>
	</properties>
	<body>
		<release version="0.9.16" date="2019-04.21"
			description="introduce new keywords to verify form fields">
			<action dev="kdoerges" type="add">
				new keyword
				verifyFieldActionChecksum: calculate the checksum of a field and
				verify against expected sum
			</action>
			<action dev="kdoerges" type="add">
				showFormFields will show
				availability of action scripts for each field
			</action>
			<action dev="kdoerges" type="add">
				new keyword verifyFieldScript:
				verify that a field has the expected additional action script
				attached
			</action>
			<action dev="kdoerges" type="add">
				new keyword storeFieldAction:
				extract the script and send it back to the calling system
			</action>
			<action dev="kdoerges" type="update">
				update pdfbox 2.0.15
			</action>
		</release>
		<release version="0.9.15" date="2019-03.19"
			description="refactor loading of resources">
			<action dev="kdoerges" type="add">
				lazy loading of pdf resource
			</action>
			<action dev="kdoerges" type="update">
				update vebtal-api
			</action>
			<action dev="nitfeu" type="add">
				add junits
			</action>
		</release>

		<release version="0.9.14" date="2019-03.01"
			description="refactoring and updating dependencies">
			<action dev="kdoerges" type="fix">
				refactor to document driver:
				PDFDriver
			</action>
			<action dev="kdoerges" type="fix">
				fix status icon in tab
			</action>
			<action dev="kdoerges" type="update">
				update dependencies
			</action>
			<action dev="kdoerges" type="update">
				update vebtal-api
			</action>
		</release>

		<release version="0.9.13" date="2019-02.08"
			description="introduce auto completion and new section for test generation">
			<action dev="kdoerges" type="add">
				use const instead of hardcoded
				return codes
			</action>
			<action dev="kdoerges" type="add">
				add autocompletion
			</action>
			<action dev="kdoerges" type="add">
				add annotation to keyword
				implementation classes
			</action>
			<action dev="kdoerges" type="add">
				section for test generation
			</action>
		</release>

		<release version="0.9.12" date="2019-01.30"
			description="provide new keywords and validate test object">
			<action dev="kdoerges" type="add">
				implement new keyword
				extractImages
			</action>
			<action dev="kdoerges" type="add">
				new keywords verifyCreationDate,
				Validate
			</action>
		</release>

		<release version="0.9.11" date="2019-01.20"
			description="refactoring and bugfixing">
			<action dev="kdoerges" type="fix">
				catch InvalidPasswordException
				while loading pdf
			</action>
			<action dev="kdoerges" type="fix">
				refactored response code
			</action>
		</release>

		<release version="0.9.10" date="2019-01.10"
			description="bugfixes">
			<action dev="kdoerges" type="update">
				repackaged
			</action>
		</release>

		<release version="0.9.9" date="2019-01.10"
			description="strengthen build process">
			<action dev="kdoerges" type="add">
				refactor build process, include
				modulname
			</action>
		</release>

		<release version="0.9.8" date="2019-01.06"
			description="new internal api system">
			<action dev="kdoerges" type="update">
				update dependency veptal-api
			</action>
		</release>

		<release version="0.9.7" date="2019-01.03"
			description="refactoring internal build process">
			<action dev="kdoerges" type="add">
				refactor build process to parent
			</action>
		</release>

		<release version="0.9.6" date="2018-12.27"
			description="update dependencies">
			<action dev="kdoerges" type="update">
				update pdfbox 2.13.0
			</action>
		</release>

		<release version="0.9.5" date="2018-11.14"
			description="refactoring release">
			<action dev="kdoerges" type="update">
				update pdfbox 2.12.0
			</action>
			<action dev="kdoerges" type="update">
				update assertj-core 3.11.1
			</action>
			<action dev="kdoerges" type="add">
				implement type for commands
			</action>
		</release>

		<release version="0.9.4" date="2018-11.12"
			description="use new config system">
			<action dev="kdoerges" type="add">
				new user config override system
			</action>
			<action dev="kdoerges" type="add">
				update pdfbox 2.11.0
			</action>
		</release>

		<release version="0.9.3" date="2018-07.31"
			description="refactoring release">
			<action dev="kdoerges" type="fix">
				refactor tab handling
			</action>
			<action dev="kdoerges" type="add">
				implement sut status
			</action>
			<action dev="kdoerges" type="add">
				implement config loader
			</action>
		</release>

		<release version="0.9.2" date="2018-07.04"
			description="clean up">
			<action dev="kdoerges" type="add">
				save start and stop time for
				keywords
			</action>
			<action dev="kdoerges" type="remove">
				remove unused code
			</action>
		</release>

		<release version="0.9.1" date="2018-06.19"
			description="refactoring and new internal tests">
			<action dev="kdoerges" type="add">
				use of TestAdaptionResource
				interface
			</action>
			<action dev="kdoerges" type="add">
				refactored JUnit PDFResource
			</action>
			<action dev="kdoerges" type="add">
				added new MetaData tests
			</action>

			<action dev="kdoerges" type="fix">
				verifytextbyarea: search text in
				area can have line break
			</action>
			<action dev="kdoerges" type="add">
				lots of junit test cases
			</action>
			<action dev="kdoerges" type="add">
				new asserts: VerifyMetaDataAssert,
				VerifyTextByAreaAssert, VerifyTextAssert
			</action>
			<action dev="kdoerges" type="add">
				new keyword VerifyTextByArea
			</action>

		</release>

	</body>
</document>